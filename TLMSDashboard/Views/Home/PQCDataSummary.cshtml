@model List<TLMSData.Models.ProductionSummary>

<form >
    <div class="form-inline">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputEmail4">Email</label>
                <input type="text" name="daterange" class="form-control w-100" value="01/01/2018 - 01/15/2018" />
            </div>
            <div class="form-group col-md-6">
                <label for="inputPassword4">Product</label>
                <input type="text" class="form-control" name="Product" placeholder="Enter your product">
            </div>
        </div>
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary mb-2 bg-blue-2">Submit</button>
    </div>
</form>

<div class="mt-3">
    <table class="table table-striped table-bordered maptable" data-provider='[{"Date":"2022-01-08T00:00:00","Hour":15,"Line":"L01","Product":"ABCXYZ","PassedQty":210,"NotPassedQty":3,"performance":{"Yield":98.59,"Throughput":210}},{"Date":"2022-01-08T00:00:00","Hour":16,"Line":"L01","Product":"ABCXYZ","PassedQty":80,"NotPassedQty":0,"performance":{"Yield":100,"Throughput":80}},{"Date":"2022-01-08T00:00:00","Hour":17,"Line":"L01","Product":"ABCXYZ","PassedQty":0,"NotPassedQty":61,"performance":{"Yield":0,"Throughput":0}},{"Date":"2022-01-08T00:00:00","Hour":19,"Line":"L01","Product":"ABCXYZ","PassedQty":0,"NotPassedQty":5,"performance":{"Yield":0,"Throughput":0}},{"Date":"2022-01-09T00:00:00","Hour":20,"Line":"L01","Product":"ABCXYZ","PassedQty":0,"NotPassedQty":4,"performance":{"Yield":0,"Throughput":0}}]'>
        <thead class="thead-blue">
 @{
                foreach (var item in Model)
                {
                    <tr>
                        <th data-name="Line">@item.ProductionLine.Line</th>
                <th data-name="Product">@item.ProductionLine.Product</th>
                <th data-name="PassedQty">@item.ProductionLine.Output</th>
                <th data-name="Date">@item.ProductionLine.InspectStart</th>
                <th data-name="NotPassedQty">@item.ProductionLine.NotGood</th>
            </tr>
            }
 }
        </thead>
    </table>
</div>